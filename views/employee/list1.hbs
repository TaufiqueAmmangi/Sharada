<h3><a class="btn btn-secondary" href="/register"><i class="fa fa-plus"></i> Create New</a> Register</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
              <th>Last Name</th>
            <th>Address</th>
              <th>College Name</th>
            <th>Mobile</th>
            <th>City</th>
            {{!-- <th></th> --}}
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.firstName}}</td>
             <td>{{this.lastName}}</td>
            <td>{{this.add}}</td>
             <td>{{this.collegeName}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
            <td>
                <a href="/register/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/register/delete/{{this._id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>